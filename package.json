{
  "name": "gsd",
  "version": "0.0.1",
  "description": "Get Stuff Done App",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsfeb26/gsd.git"
  },
  "keywords": [
    "babel",
    "express",
    "isomorphic",
    "node",
    "react",
    "redux",
    "universal"
  ],
  "author": "Jason Stinson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsfeb26/gsd/issues"
  },
  "homepage": "https://github.com/jsfeb26/gsd#readme",
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm run test -- --watch",
    "dev-server": "webpack-dev-server --config webpack.config.dev.js --hot --port 5001",
    "server": "nodemon --delay 1 --ignore node_modules --ignore dist --verbose --exec babel-node src/server",
    "server-api": "nodemon --delay 1 --ignore node_modules --ignore dist --verbose --exec babel-node api/api",
    "start-dev-api": "node ./node_modules/better-npm-run start-dev-api",
    "start": "npm run dev-server & npm run server & npm run server-api"
  },
  "betterScript": {
    "start-dev": {
      "command": "node ./bin/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": "5000",
        "APIPORT": "5050"
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": "5050"
      }
    },
    "watch-client": {
      "command": "node ./bin/api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": "5050"
      }
    }
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "http-proxy": "^1.11.2",
    "mongoose": "^4.1.3",
    "pretty-error": "^1.2.0",
    "qs": "^5.0.0",
    "query-string": "^2.4.1",
    "react": "^0.13.3",
    "react-document-meta": "^1.1.0",
    "react-redux": "^2.1.2",
    "react-router": "v1.0.0-beta2",
    "redux": "^3.0.0",
    "redux-thunk": "^0.1.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.4.0",
    "webpack-isomorphic-tools": "^0.8.8"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "better-npm-run": "0.0.2",
    "chai": "^3.2.0",
    "chai-http": "^1.0.0",
    "json-loader": "^0.5.2",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  }
}
